// prisma/schema.prisma

datasource db {
  provider = "sqlite"
  url = env("DATABASE_URL")
}

model Task {
  id          Int      @id @default(autoincrement())
  lotteryDrawResult       String
  lotteryDrawTime String?
  lotteryGameNum String?
}

model Project {
  id          Int      @id @default(autoincrement())
  red       String
  blue String?
  date String?
}


// 添加生成器配置
generator client {
  provider = "prisma-client-js"
}
