SELECT * FROM "Project";

DROP TABLE "Project";

CREATE TABLE "Project" (
    "id" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    "red" TEXT NOT NULL,
    "blue" TEXT,
    "date" TEXT
);
